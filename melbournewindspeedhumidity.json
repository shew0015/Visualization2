{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/shew0015/Visualization2/main/weather_melbourne_windspeed_humidity_pressure_9am.csv"
  },
  "width": 790,
  "height": 300,
  "layer": [
    {
      "mark": {
        "type": "point",
        "filled": true,
        "size": 50,
        "color": "#d63638"
      },
      "encoding": {
        "x": {
          "field": "WindSpeed9am",
          "type": "quantitative",
          "title": "Wind Speed (km/hr)"
        },
        "y": {
          "field": "Humidity9am",
          "type": "quantitative",
          "title": "Humidity (%)"
        }
      }
    },
    {
      "transform": [
        {
          "regression": "Humidity9am",
          "on": "WindSpeed9am",
          "params": true
        },
        {
          "calculate": "'RÂ²: '+format(datum.rSquared, '.2f')",
          "as": "R2"
        }
      ],
      "mark": {
        "type": "text",
        "color": "#8a2424",
        "x": "width",
        "align": "right",
        "y": -5
      },
      "encoding": {
        "text": {
          "type": "nominal",
          "field": "R2"
        }
      }
    }
  ]
}